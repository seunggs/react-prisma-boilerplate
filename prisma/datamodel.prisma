type User {
  id: ID! @unique
  updatedAt: DateTime!
  createdAt: DateTime!
  email: String! @unique
  hasPassword: Boolean
  hasSocialLogin: Boolean
  name: String
  username: String @unique
  picture: String
  twitter: String
  projects: [Project]! @relation(name: ProjectToUser, onDelete: CASCADE)
}

type File {
  id: ID! @unique
  createdAt: DateTime!
  updatedAt: DateTime!
  filename: String!
  mimetype: String!
  encoding: String!
  url: String! @unique
}